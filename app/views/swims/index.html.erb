<p id="notice"><%= notice %></p>

<h1>Listing Swims</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>Duration</th>
      <th>Distance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @swims.each do |swim| %>
      <tr>
        <td><%= swim.start %></td>
        <td><%= swim.duration %></td>
        <td><%= swim.distance %></td>
        <td><%= link_to 'Show', swim %></td>
        <td><%= link_to 'Edit', edit_swim_path(swim) %></td>
        <td><%= link_to 'Destroy', swim, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Swim', new_swim_path %>
